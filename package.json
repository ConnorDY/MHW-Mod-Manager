{
  "name": "mhw-mod-manager",
  "version": "1.3.3",
  "author": {
    "name": "Connor Younglund",
    "email": "cdy@airmail.cc",
    "url": "https://github.com/ConnorDY"
  },
  "homepage": "./",
  "main": "src/electron/starter.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "electron-builder install-app-deps",
    "electron": "electron ./src/electron/starter.js",
    "electron-dev": "cross-env DEV=true npm run electron",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder build",
    "electron-pack:windows": "npm run electron-pack -- -w",
    "electron-pack:mac": "npm run electron-pack -- -m"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "prettier": {
    "arrowParens": "always",
    "singleQuote": true,
    "tabWidth": 2
  },
  "build": {
    "appId": "link.cdy.mhw-mod-manager",
    "copyright": "Copyright Â© 2020 ${author}",
    "extends": null,
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "src/electron/*.js"
    ],
    "mac": {
      "category": "public.app-category.utilities"
    },
    "productName": "MHW Mod Manager"
  },
  "dependencies": {
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "fs-extra": "^8.1.0",
    "jszip": "^3.2.2",
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-file-drop": "^0.2.8",
    "react-scripts": "3.3.0",
    "react-transition-group": "^4.3.0",
    "typeface-roboto": "0.0.75"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/electron": "^1.6.10",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^25.1.1",
    "@types/jszip": "^3.1.7",
    "@types/node": "^13.5.2",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "cross-env": "^7.0.0",
    "electron": "^7.1.10",
    "electron-builder": "^22.3.2",
    "prettier": "^1.19.1",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
